    "from time import sleep\n",
    "from random import randint\n",
    "\n",
    "def sleep_for(n):\n",
    "    print('before sleep id % d' % randint(0, 100))\n",
    "    sleep(n)\n",
    "    print('after sleep id % d' % randint(0, 100))\n",
    "\n",
    "n = 3\n",
    "sleep_for(n)"
   ]
